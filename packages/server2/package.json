  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "copy-assets": "ts-node tools/copyAssets",
    "tsc": "tsc",
    "build": "run-s tsc copy-assets",
    "preview": "tsc && env-cmd -f .env.production.local node build/index.js",
    "dev": "run-p start:dev",
    "start:dev": "env-cmd -f .env nodemon --exec ts-node src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "gen": "graphql-codegen --config codegen.yml -w",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "generate": "graphql-codegen --config codegen.preview.yml"
  },
